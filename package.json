{
  "name": "storybook-addon-vue-mdx",
  "version": "1.0.2",
  "description": "Use Vue components inside MDX files, as if they were React components.",
  "keywords": [
    "storybook-addons",
    "organize",
    "documentation",
    "vue",
    "mdx"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sidnioulz/storybook-addon-vue-mdx.git"
  },
  "author": "Steve Dodier-Lazaro <sidnioulz@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./jsx-dev-runtime": {
      "require": "./dist/jsx-runtime.js",
      "import": "./dist/jsx-runtime.mjs",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-runtime": {
      "require": "./dist/jsx-runtime.js",
      "import": "./dist/jsx-runtime.mjs",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./manager": {
      "require": "./dist/manager.js",
      "import": "./dist/manager.mjs",
      "types": "./dist/manager.d.ts"
    },
    "./preset": {
      "require": "./dist/preset.js",
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts"
    },
    "./preview": {
      "require": "./dist/preview.js",
      "import": "./dist/preview.mjs",
      "types": "./dist/preview.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/manager.ts",
      "./src/preset.ts",
      "./src/preview.ts",
      "./src/jsx-runtime.ts"
    ]
  },
  "scripts": {
    "build-storybook": "storybook build",
    "clean": "rimraf ./dist",
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache . --ext .ts,.tsx,.js,.jsx,.vue,.cjs",
    "lint:fix": "yarn lint --fix",
    "pack:local": "yarn pack --out storybook-addon-vue-mdx-$(date +%s).tgz",
    "prebuild": "yarn clean",
    "prepack:local": "yarn build",
    "prepare": "husky install",
    "release": "npm run build && auto shipit",
    "start": "run-p build:watch 'storybook'",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@commitlint/config-conventional": "^17.6.5",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/builder-vite": "^8.0.0",
    "@storybook/components": "^8.0.0",
    "@storybook/manager": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview-api": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/theming": "^8.0.0",
    "@storybook/types": "^8.0.0",
    "@storybook/vue3": "^8.0.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^11.0.3",
    "auto": "^11.1.6",
    "boxen": "^5.0.1",
    "commitlint": "^17.6.5",
    "dedent": "^0.7.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.9",
    "storybook": "^8.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "vue": "^3.4.31",
    "vuestic-ui": "^1.6.6",
    "zx": "^7.2.3"
  },
  "peerDependencies": {
    "@storybook/blocks": "^8.0.0",
    "@storybook/builder-vite": "^8.0.0",
    "@storybook/components": "^8.0.0",
    "@storybook/core-events": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview-api": "^8.0.0",
    "@storybook/theming": "^8.0.0",
    "@storybook/types": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vue": "^3.4.31"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Vue support for MDX",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "dependencies": {
    "veaury": "^2.4.2"
  }
}
