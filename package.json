{
  "name": "storybook-addon-vue-mdx",
  "version": "1.0.6",
  "description": "Use Vue components inside MDX files, as if they were React components.",
  "keywords": [
    "storybook-addons",
    "organize",
    "documentation",
    "vue",
    "mdx"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sidnioulz/storybook-addon-vue-mdx.git"
  },
  "author": "Steve Dodier-Lazaro <sidnioulz@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "require": "./dist/jsx-runtime.js",
      "import": "./dist/jsx-runtime.mjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "require": "./dist/jsx-runtime.js",
      "import": "./dist/jsx-runtime.mjs"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "require": "./dist/manager.js",
      "import": "./dist/manager.mjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js",
      "import": "./dist/preset.mjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "require": "./dist/preview.js",
      "import": "./dist/preview.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "bundler": {
    "exportEntries": [
      "src/index.ts",
      "src/jsx-runtime.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "scripts": {
    "build-storybook": "storybook build",
    "clean": "rimraf ./dist",
    "build": "tsup",
    "build:watch": "pnpm build --watch",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache . --ext .ts,.tsx,.js,.jsx,.vue,.cjs",
    "lint:fix": "pnpm lint --fix",
    "pack:local": "pnpm pack --out storybook-addon-vue-mdx-$(date +%s).tgz",
    "prebuild": "pnpm clean",
    "prepack:local": "pnpm build",
    "prepare": "husky",
    "release": "npm run build && auto shipit",
    "start": "run-p build:watch 'storybook'",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@storybook/addon-docs": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-links": "next",
    "@storybook/builder-vite": "next",
    "@storybook/components": "next",
    "@storybook/core-events": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/manager": "next",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/types": "next",
    "@storybook/vue3": "next",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^19.8.1",
    "dedent": "^1.6.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "storybook": "next",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vue": "^3.5.13",
    "vuestic-ui": "^1.10.3",
    "zx": "^8.5.4"
  },
  "peerDependencies": {
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/builder-vite": "^9.0.0",
    "@storybook/components": "^9.0.0",
    "@storybook/core-events": "^9.0.0",
    "@storybook/types": "^9.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vue": "^3.4.31"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=20"
  },
  "storybook": {
    "displayName": "Vue support for MDX",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "dependencies": {
    "veaury": "^2.6.2"
  }
}
